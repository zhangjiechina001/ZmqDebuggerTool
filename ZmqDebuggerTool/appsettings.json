{
  "RequestAddress": "tcp://localhost:3000",
  "SubscriberAddress": "tcp://localhost:5000",
  "Commands": {
    "设置参数_单次测量": {
      "theme": "setMeasureParams",
      //不同的控制参数，单次测量、采集光谱
      "measureMode": "单次测量",
      "sampleInformation": "JGSC-BTF",
      //测量次数
      "value": 200
    },
    "设置参数_采集光谱": {
      "theme": "setMeasureParams",
      //不同的控制参数，单次测量、采集光谱
      "measureMode": "采集光谱",
      "sampleInformation": "JGSC-BTF",
      //测量次数
      "value": 200
    },
    "启动测量": {
      "theme": "measureControl",
      //startMeasure stopMeasure
      "param": "startMeasure"
    },
    "停止测量": {
      "theme": "measureControl",
      //startMeasure stopMeasure
      "param": "stopMeasure"
    }
  }
}
